{
    "manifest_version": 2,
    "name": "웨일 전용 이미지 검색 앱",
    "version": "1.0.0",
    "description": "search mong",

    "sidebar_action": {
        "default_page": "sidebar/sidebar.html",
        "default_icon": "images/search_mong.png",
        "default_title": "서치몽",
        "use_navigation_bar": true,
        "mobile_user_agent": true
    },
    "background": {
        "scripts" : ["js/jquery-3.4.1.min.js","js/background.js"]
    },
    "icons" : {
      "16": "images/search_mong.png",
      "48": "images/search_mong.png",
      "128": "images/search_mong.png"
    },
    "content_security_policy": "script-src 'self' https://code.jquery.com/jquery-3.4.1.js; object-src 'self'",
    "permissions": ["downloads","tabs","desktopCapture","clipboardRead","clipboardWrite", "contextMenus","storage"]
}
